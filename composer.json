{
  "name": "crawly/inscricao-estadual-laravel",
  "description": "Lib de validação de inscrição estadual no laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Filipe Tagliatti",
      "email": "filipetagliatti@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Crawly\\InscricaoEstadualLaravel\\InscricaoEstadualValidatorProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Crawly\\InscricaoEstadualLaravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crawly\\InscricaoEstadualLaravel\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.2",
    "thiagocfn/inscricaoestadual": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.*",
    "phpmd/phpmd": "2.8.*",
    "phpstan/phpstan": "0.12.*",
    "sebastian/phpcpd": "4.1.*",
    "friendsofphp/php-cs-fixer": "2.17.*",
    "orchestra/testbench": "4.*"
  },
  "scripts": {
    "phpcs": "php-cs-fixer fix src/",
    "phpcpd": "phpcpd --verbose src/",
    "phpmd": "phpmd src/ text codesize,unusedcode,design,phpmd-ruleset.xml",
    "phpstan": "phpstan analyse -l 4 src/",
    "coverage": "phpunit --whitelist src/ --coverage-html cover/ --coverage-text",
    "test": "phpunit",
    "analysis": [
      "@phpcs",
      "@phpcpd",
      "@phpmd",
      "@phpstan"
    ]
  }
}
